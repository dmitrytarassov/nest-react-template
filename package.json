    "react-modal": "^3.15.1",
    "react-resizable": "^3.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "styled-components": "^5.3.5",
    "swiper": "^8.1.4",
    "swr": "^1.3.0",
    "typeorm": "^0.3.6"
