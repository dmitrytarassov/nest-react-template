{
  "name": "blog-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:local": "NODE_ENV=production nest build && cd src/client; NODE_ENV=production next build",
    "build": "export NODE_OPTIONS=--max_old_space_size=2048; NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=1024\" nest build && cd src/client; NODE_ENV=production next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prod:start": "pm2 start --name=\"kinoscope\"",
    "prod:stop": "pm2 stop kinoscope",
    "prod:restart": "yarn prod:staop; yarn prod:start",
    "log": "pm2 log",
    "status": "pm2 status",
    "console:dev": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "console": "node dist/server/console.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "mocks:generate": "ts-node -r tsconfig-paths/register src/lib/mocks/generate.ts",
    "docker:start:local": "docker-compose up -d",
    "docker:down:local": "docker-compose down",
    "docker:restart:local": "yarn docker:down:local && yarn docker:start:local"
  },
  "dependencies": {
    "@fusionworks/ra-data-nest-crud": "^1.0.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.4",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@sindresorhus/transliterate": "^1.5.0",
    "add": "^2.0.6",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "classnames": "^2.3.1",
    "connect-mongo": "^4.6.0",
    "date-fns": "^2.28.0",
    "express-session": "^1.17.3",
    "leaflet": "^1.8.0",
    "libphonenumber-js": "^1.9.53",
    "lodash": "^4.17.21",
    "lorem-ipsum": "^2.0.4",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.4",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "nestjs-console": "^7.0.1",
    "next": "^12.1.5",
